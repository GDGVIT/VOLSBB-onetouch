{
  "manifest_version": 2,

  "name": "VOLSBB oneTouch",
  "description": "Web port of VOLSBB one touch ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*.prontonetworks.com/*"
  ],
  "background":{
    "scripts":[
      "scripts/background.js"
    ]
  }
}
